pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- keyed in from the tutorial in pico-zine #1
-- ...with bug fixes
function make_smoke(x,y,init_size,col)
  local s={}
  s.x = x
  s.y = y
  s.col = col
  s.width = init_size
  s.width_final = init_size + rnd(3)  + 1
  s.t = 0
  s.max_t = 30 + rnd(10)
  s.dx = (rnd(.8) - .4)
  s.dy = (rnd(.05))
  s.ddy = .02
  add(smoke, s)
  return s
end

function _init()
  smoke = {}
  cursor_x = 50
  cursor_y = 50
  color = 7
end

function move_smoke(sp)
  -- remove it for next time
  if (sp.t > sp.max_t) then
    del(smoke,sp)
  end

  -- grows for the first 15 frames
  if (sp.t > sp.max_t - 15) then
    sp.width +=1
    sp.width = min(sp.width,sp.width_final)
  end

  sp.x = sp.x + sp.dx
  sp.y = sp.y - sp.dy   -- it's smoke so it goes UP
  sp.dy= sp.dy + sp.ddy
  sp.t = sp.t + 1
end

function _update ()
  foreach(smoke, move_smoke)

  if btn(0,0) then cursor_x -= 1 end
  if btn(1,0) then cursor_x += 1 end
  if btn(2,0) then cursor_y -= 1 end
  if btn(3,0) then cursor_y += 1 end
  if btn(4,0) then color = flr(rnd(16)) end

  make_smoke(cursor_x,cursor_y,rnd(4),color)
end

function draw_smoke(s)
  circfill(s.x, s.y,s.width, s.col)
end

function _draw()
  cls()
  foreach(smoke, draw_smoke)
end

